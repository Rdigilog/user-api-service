{
  "name": "admin-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seeders/admin.seed.ts && ts-node prisma/seeders/country.seeder.ts && ts-node prisma/seeders/plan.seed.ts && ts-node prisma/seeders/role.seed.ts",
    "db:studio": "prisma studio",
    "deploy": "./scripts/deploy.sh",
    "health:check": "curl -f http://localhost:3000/api/v1/health || exit 1",
    "health:db": "curl -f http://localhost:3000/api/v1/health/db || exit 1",
    "health:migration": "curl -f http://localhost:3000/api/v1/health/migration || exit 1",
    "db:pull": "prisma db pull --schema=prisma/schema.prisma && prisma-case-format --file prisma/schema.prisma --table-case pascal,singular --field-case camel,singular --map-table-case snake --map-field-case snake --enum-case pascal,singular --map-enum-case snake && prisma generate --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.901.0",
    "@bull-board/express": "^6.12.7",
    "@bull-board/nestjs": "^6.12.7",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.11",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.15.0",
    "@prisma/extension-read-replicas": "^0.4.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "bullmq": "^5.41.7",
    "cache-manager": "^6.4.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "fast-csv": "^5.0.2",
    "form-data": "^4.0.2",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-import": "^1.0.5",
    "qrcode": "^1.5.4",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.4.5",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.15.0",
    "prisma-case-format": "^2.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
